@model Travel.Web.Data.Entities.UserEntity

@{
    ViewData["Title"] = "DetailsUserTravels";
}


    <link href="https://cdn.datatables.net/1.10.19/css/jquery.datatables.min.css" rel="stylesheet" />




<div class="row">
    <div class="col-md-12">
        <div class="panel panel-default">
            <div class="panel-heading">
                <h3 class="panel-title">List Travels Employee</h3>
             </div>

            <div class="panel-body">
                <table class="table table-hover table-responsive table-striped" id="MyTable">
                    <thead>
                        <tr>
                            <th>
                                @Html.DisplayNameFor(model => model.Travels.FirstOrDefault().Name)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.Travels.FirstOrDefault().StartDate)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.Travels.FirstOrDefault().EndDate)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.Travels.FirstOrDefault().City)
                            </th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var item in Model.Travels)
                        {
                            <tr>
                                <td>
                                    @Html.DisplayNameFor(model => item.Name)
                                </td>
                                <td>
                                    @Html.DisplayNameFor(model => item.StartDate)
                                </td>
                                <td>
                                    @Html.DisplayNameFor(model => item.EndDate)
                                </td>
                                <td>
                                    @Html.DisplayNameFor(model => item.City)
                                </td>
                                <td>
                                    <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                                </td>
                            </tr>
                        }
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>



